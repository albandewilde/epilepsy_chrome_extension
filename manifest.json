{
    "name": "Epilepsy",
    "description" : "Display RGB colors on background",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "epilepsy.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+E"
            },
            "description": "Start the epilepsy"
        }
    }
}
